<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--通用Bar-->
    <declare-styleable name="HeadBar">
        <attr name="centerTitle" format="string" />
        <attr name="centerTitle_color" format="color" />
        <attr name="centerTitle_size" format="dimension" />
        <attr name="rightTitle" format="string" />
        <attr name="rightTitle_color" format="color" />
        <attr name="rightTitle_size" format="dimension" />
        <attr name="rightImage" format="reference" />
    </declare-styleable>
    <!--流式布局-->
    <declare-styleable name="TagFlowLayout">
        <attr name="max_select" format="integer" />
        <attr name="tag_gravity">
            <enum name="left" value="-1" />
            <enum name="center" value="0" />
            <enum name="right" value="1" />
        </attr>
    </declare-styleable>
</resources>